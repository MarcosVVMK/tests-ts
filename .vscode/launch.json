    "version": "0.2.0",
    "configurations": [
        {
            "name": "Strapi v4 Debug",
            "type": "node",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "runtimeExecutable": "node",
            "runtimeArgs": ["--inspect"],
            "skipFiles": ["<node_internals>/**"],
            "program": "${workspaceFolder}/node_modules/@strapi/strapi/bin/strapi.js",
            "args": ["develop"],
            "autoAttachChildProcesses": true,
            "console": "integratedTerminal"
        }
    ]
}